<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.azwar.finalproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainMenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".KatalogActivity"
            android:label="Katalog Arisan"
            android:parentActivityName=".MainMenuActivity" />
        <activity
            android:name=".DaftarKelompokArisanActivity"
            android:label="Daftar Kelompok Arisan"
            android:parentActivityName=".MainMenuActivity" />
        <activity
            android:name=".MenuKetuaArisanActivity"
            android:label="Menu Ketua Arisan"
            android:parentActivityName=".MainMenuActivity" />
        <activity
            android:name=".KategoriActivity"
            android:label="Kategori"
            android:parentActivityName=".KatalogActivity" />
        <activity
            android:name=".HasilPencarianActivity"
            android:label="Hasil Pencarian"
            android:parentActivityName=".KatalogActivity"></activity>
    </application>

</manifest>